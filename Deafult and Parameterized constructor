// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class name
{
    int x,y;
    public : name ()//Deafult constructor
    {
        x=10;
        y=20;
    }
    
      name (int a,int b)//parametarized constructor
    {
        x=a;
        y=b;
    }
    void sum()//calls deafult constructor
    {
        cout<<x+y<<endl;
    }
    void sub()//calls parametrized constructor
    {
        cout<<x-y<<endl;
    }
    };
     int main()
    {
        name obj,obj1(30,20);
        obj.sum();
        obj1.sub();
       
    }

