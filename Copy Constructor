// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
class name
{
int x,y;
public : name (int a,int b)
{
    x=a;
    y=b;
}
void sum()
{
    cout<<x+y<<endl;
}
name(name &obj)
{
    x=obj.x;
    y=obj.y;
}
};
int main()
{
name obj(10,20);
obj.sum();
name obj1=obj;
obj1.sum();
}
